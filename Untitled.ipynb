    "line = list(range(30))\n",
    "even = []\n",
    "odd = []\n",
    "patron = 0\n",
    "\n",
    "while patron in line:\n",
    "    if patron % 2 == 0:\n",
    "        even.append(patron)\n",
    "    else:\n",
    "        odd.append(patron)\n",
    "    if even and odd == 15:\n",
    "        break\n",
    "    patron += 1"
